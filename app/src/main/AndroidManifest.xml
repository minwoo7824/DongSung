roid:name=".Activitys.SpecialActivity"            android:configChanges="screenSize|orientation" />        <receiver            android:name=".Commons.NetworkChangeReceiver"            android:label="NetworkChangeReceiver">            <intent-filter>                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>            </intent-filter>        </receiver>        <service            android:name=".Commons.Https.MyFirebaseMessagingService"            android:stopWithTask="false">            <intent-filter>                <action android:name="com.google.firebase.MESSAGING_EVENT" />            </intent-filter>        </service>        <service android:name=".Commons.UpdateService" />        <receiver            android:name=".Commons.Starter"            android:enabled="true"            android:permission="android.permission.RECEIVE_BOOT_COMPLETED">            <intent-filter>                <action android:name="android.intent.action.BOOT_COMPLETED" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </receiver>        <service android:name=".Commons.NotDeathService" />        <service android:name=".Commons.MediaPlayService" />        <activity            android:name=".SplashActivity"            android:screenOrientation="landscape">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".Activitys.Insert.BinsoInsertActivity" />        <activity android:name=".Activitys.ModeSelectActivity" />        <activity            android:name=".Activitys.LoginActivity"            android:screenOrientation="landscape" />        <activity android:name=".Activitys.Insert.DeceasedInsertActivity" />        <activity            android:name=".Activitys.DeceasedImgActivity"/>        <activity android:name=".Activitys.Insert.GeneralInsertActivity" />        <activity android:name=".Activitys.Insert.EntryInsertActivity" />        <activity android:name=".Activitys.BinsoActivity" />        <activity android:name=".Activitys.GeneralActivity" />        <activity            android:name=".Activitys.TestActivity"            android:hardwareAccelerated="true" />        <activity            android:name=".Activitys.InternetGeneralActivity"            android:hardwareAccelerated="false" />        <activity android:name=".Activitys.InternetBinsoActivity" />        <activity            android:name=".Activitys.WaitingActivity"            android:hardwareAccelerated="true" />        <activity android:name=".Activitys.InternetSangGaActivity" />        <activity android:name=".Activitys.InternetEntryActivity" />        <activity android:name=".Activitys.BaseFontActivity" />    </application></manifest>